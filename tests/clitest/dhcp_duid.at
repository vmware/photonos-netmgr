AT_BANNER([netmgr tests start])

AT_SETUP([dhcp_duid])
AT_KEYWORDS([dhcp_duid])
m4_define([TEST_DUID], [00:01:11:22:33:44:55:66:77:88])
m4_define([RESET_DUID], [''])

AT_CHECK([$abs_top_builddir/tools/netmgr/netmgr dhcp_duid --get],
[2])

AT_CHECK([$abs_top_builddir/tools/netmgr/netmgr dhcp_duid --set --duid TEST_DUID],
[0],
[],
[],
[$abs_top_builddir/tools/netmgr/netmgr dhcp_duid --set --duid RESET_DUID])

AT_CHECK([$abs_top_builddir/tools/netmgr/netmgr dhcp_duid --get],
[0],
[DUID=TEST_DUID
],
[],
[$abs_top_builddir/tools/netmgr/netmgr dhcp_duid --set --duid RESET_DUID])

AT_CHECK([$abs_top_builddir/tools/netmgr/netmgr dhcp_duid --set --duid RESET_DUID],
[0])

AT_CLEANUP
